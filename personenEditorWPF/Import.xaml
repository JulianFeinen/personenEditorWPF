<Window x:Class="PersonenEditorWPF.winImport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PersonenEditorWPF" Loaded="Import_Loaded"
        mc:Ignorable="d" WindowStartupLocation="CenterScreen"
        Title="Import" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="tbImport" Text="D:\Personal\bearbeiter.csv" Height="23" TextWrapping="Wrap" Width="420" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button x:Name="btnImportieren" Click="btnImportieren_Click" Content="Importieren" Width="100" Height="30" FontSize="15" Margin="10,100,10,10"></Button>
    </Grid>
</Window>
